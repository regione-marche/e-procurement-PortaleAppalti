		<action name="importPrezziUnitari" class="importPrezziUnitariAction" method="{1}">
			<result name="success" type="dispatcher">
				/WEB-INF/plugins/ppgare/aps/jsp/internalServlet/garetel/prezzi_unitari/prezziUnitariImportResult.jsp
			</result>
			<result name="input" type="chain">openPageImportPrezziUnitari</result>
			<result name="cancel" type="chain">openPageOffTelPrezziUnitari</result>
			<interceptor-ref name="fileUpload">
				<param name="maximumSize">15728640</param>
			</interceptor-ref>

		<band height="70" splitType="Stretch">
			<staticText>
				<reportElement positionType="Float" x="0" y="1" width="483" height="58"/>
				<box rightPadding="5"/>
				<textElement textAlignment="Justified" verticalAlignment="Top" lineSpacing="1_1_2">
					<font fontName="Arial" size="11" isBold="true"/>
				</textElement>
				<text><![CDATA[Domanda di iscrizione agli elenchi degli operatori economici per l'affidamento di lavori, forniture e servizi ai sensi degli articoli 36 e 63 del D.Lgs. n. 50 del 2016 e s.m.i., con autocertificazioni e dichiarazioni richieste ai fini dell’ammissione.]]></text>
			</staticText>
		</band>
		<band height="37">
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="0" width="483" height="37"/>
				<textElement textAlignment="Justified">
					<font fontName="Arial" size="11"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Il/La sottoscritto/a "
+
($P{LIBERO_PROFESSIONISTA}
    ? ($F{cognomeLiberoProfessionista} + " " + $F{nomeLiberoProfessionista} )
    : (($F{cognome} == null ? "____________________" : $F{cognome} + " ") + ($F{nome} == null ? "____________________" : $F{nome}))
) +
", nato/a a "
+
($P{LIBERO_PROFESSIONISTA}
    ? $F{comuneNascitaLiberoProfessionista}
    : $F{comuneNascita}
) +
($P{LIBERO_PROFESSIONISTA}
    ? ($F{provinciaNascitaLiberoProfessionista} == null ? " prov. ________"
      : " prov. " + $F{provinciaNascitaLiberoProfessionista})
    : ($F{provinciaNascita} == null ? " prov. ________"
        : " prov. " + $F{provinciaNascita})
) +
($P{LIBERO_PROFESSIONISTA}
    ? " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascitaLiberoProfessionista})
    : " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascita})
) +
($P{LIBERO_PROFESSIONISTA}
    ? ", in qualità di Libero Professionista"
    : ", in qualità di " + $F{qualifica} + " dell'impresa " + $F{ragioneSociale} + ", " +
      "con sede in " + $F{comuneSedeLegale} + " (" + $F{provinciaSedeLegale} + "), " + $F{indirizzoSedeLegale} + " " +
      "n " + $F{numCivicoSedeLegale} + " " +
      "(" +
      "n tel " + ($F{telefonoRecapito} == null ? "" : $F{telefonoRecapito}) + " " +
      "n fax " + ($F{faxRecapito} == null ? "" : $F{faxRecapito}) + " " +
      "email " + ($F{emailRecapito} == null ? "" : $F{emailRecapito}) +
      ") " +
      "email PEC (posta elettronica certificata) " + ($F{pecRecapito} == null ? "" : $F{pecRecapito}) + " " +
      "con codice fiscale n. " + ($F{codiceFiscale} == null ? "________________" : $F{codiceFiscale}) + " " +
      "e partita IVA n. " + ($F{partitaIVA} == null ? "________________" : $F{partitaIVA}) + " "
)]]></textFieldExpression>
			</textField>
		</band>

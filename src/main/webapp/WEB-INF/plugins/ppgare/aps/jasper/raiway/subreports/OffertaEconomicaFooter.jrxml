				<textElement textAlignment="Justified">
					<font fontName="Arial"/>
				</textElement>
				<text><![CDATA[che la presente offerta non vincolerà in alcun modo Rai Way;]]></text>
			</staticText>
			<staticText>
				<reportElement positionType="Float" x="17" y="356" width="10" height="45"/>

			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="313" y="0" width="170" height="63" uuid="0f8979bf-91dd-4143-b582-8d01b44ef27a"/>
				<textElement textAlignment="Left" markup="none">
					<font fontName="Arial" size="11" isBold="false" pdfFontName="Helvetica"/>
				</textElement>
				<textFieldExpression><![CDATA["All'Università degli Studi di Padova\n" +
"Ufficio acquisti\n" +
"Piazza Antenore, 3\n" +
"35123 Padova"]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement positionType="Float" x="284" y="63" width="199" height="21" uuid="1b2f197e-92e3-40c2-8297-45eed3fdf4a9"/>
				<textElement>
					<font fontName="Arial"/>
				</textElement>
				<text><![CDATA[]]></text>
			</staticText>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="0" y="84" width="483" height="15" uuid="5bda6346-bf14-4387-b8d4-d1ef5b514d59"/>
				<textElement textAlignment="Justified" markup="none">
					<font fontName="Arial" size="11" isBold="false" pdfFontName="Helvetica"/>
				</textElement>
				<textFieldExpression><![CDATA["Rinnovo della dichiarazione sostitutiva di certificazioni e di atti notori al fine dell’abilitazione al Mercato  elettronico dell’Università degli Studi di Padova."]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" isPrintRepeatedValues="false" x="0" y="114" width="483" height="30" uuid="895c3640-251a-4b5a-86d0-c8959f946f5a">
					<printWhenExpression><![CDATA[$P{RTI}==false]]></printWhenExpression>
				</reportElement>
				<textElement textAlignment="Justified" verticalAlignment="Bottom" markup="html">
					<font fontName="Arial" size="11"/>
					<paragraph lineSpacing="Single"/>
				</textElement>
				<textFieldExpression><![CDATA["Il/La sottoscritto/a "
+
($P{LIBERO_PROFESSIONISTA}
    ? $F{ragioneSociale}
    : (($F{cognome} == null ? "" : $F{cognome} + " ") + ($F{nome} == null ? "" : $F{nome}))
) +
", nato/a a "
+
($P{LIBERO_PROFESSIONISTA}
    ? $F{comuneNascitaLiberoProfessionista}
    : $F{comuneNascita}
) +
($P{LIBERO_PROFESSIONISTA}
    ? ($F{provinciaNascitaLiberoProfessionista} == null ? ""
      : " (" + $F{provinciaNascitaLiberoProfessionista} + ")")
    : ($F{provinciaNascita} == null ? ""
        : " (" + $F{provinciaNascita} + ")")
) +
($P{LIBERO_PROFESSIONISTA}
    ? " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascitaLiberoProfessionista})
    : " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascita})
) +
($P{LIBERO_PROFESSIONISTA}
    ? ", in qualità di Libero Professionista"
    : ", in qualità di " + $F{qualifica} + " dell'interessato " + $F{ragioneSociale}
) +
($F{partitaIVA} != null ? ", partita IVA " + $F{partitaIVA} : "") +
", codice fiscale " + $F{codiceFiscale} +
", con sede legale in " +
$F{indirizzoSedeLegale} + " " + $F{numCivicoSedeLegale} +
", " + $F{capSedeLegale} + " " + $F{comuneSedeLegale} +
($F{provinciaSedeLegale} == null ? "" : " (" + $F{provinciaSedeLegale} + ") iscritto al Mercato elettronico - Sezione Revisori Contabili dell'Università degli Studi di Padova")]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="0" y="178" width="483" height="52" uuid="67531dc2-311a-4c51-b80a-2a8f1eaa08b2"/>
				<textElement textAlignment="Justified">
					<font fontName="Arial" size="11"/>
				</textElement>
				<textFieldExpression><![CDATA["rinnovo della dichiarazione sostitutiva di certificazioni e di atti notori al fine dell’abilitazione al Mercato elettronico dell’Università degli Studi di Padova e consapevole delle sanzioni penali previste dall'art. 76 del D.P.R. 445/2000 e ss.mm., per le ipotesi di falsità in atti e dichiarazioni mendaci ivi indicate, dichiara quanto segue:"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="22" y="239" width="16" height="15" uuid="0680473a-9426-4014-91be-ce044a68e8b7"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial" size="11"/>
				</textElement>
				<textFieldExpression><![CDATA["-"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="38" y="239" width="445" height="15" uuid="1b1e1ee7-02e8-4097-a46f-69365f961eb8"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial" size="11"/>
				</textElement>
				<textFieldExpression><![CDATA["che nei confronti dell’impresa non sussiste alcuna delle circostanze di esclusione di cui agli artt. 94 e 95 del D.Lgs 36/2023 e ssmmi;"]]></textFieldExpression>
			</textField>
			<frame>
				<reportElement positionType="Float" x="0" y="254" width="483" height="5" uuid="f977d5c9-e7cc-4acc-bbf7-22d566550dc7"/>
			</frame>

				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[Domanda di iscrizione agli elenchi degli operatori economici per l'affidamento di lavori ai sensi degli articoli 57, 122 e 125 del D.Lgs.163/2006 e 332 del D.P.R. 207/2010, con autocertificazioni e dichiarazioni richieste ai fini dell’ammissione.]]></text>
			</staticText>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="202" width="483" height="44"/>
				<textElement textAlignment="Justified" verticalAlignment="Bottom" lineSpacing="1_1_2" markup="html">
					<font size="11"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Il/la sottoscritto/a " + $F{cognome} + " " + $F{nome} +
", nato/a a " + $F{comuneNascita} +
" il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascita}) +
", in qualità di " + $F{qualifica} + " dell'impresa " + $F{ragioneSociale} +
" con sede in " +  $F{comuneSedeLegale} + " (" + ("Italia".equalsIgnoreCase($F{nazioneSedeLegale})? $F{provinciaSedeLegale} : $F{nazioneSedeLegale}) + "), " +
$F{indirizzoSedeLegale} + ("".equals($F{numCivicoSedeLegale})?"" : " n. " + $F{numCivicoSedeLegale})  +
($F{telefonoRecapito} != null ? ", n. tel. " + $F{telefonoRecapito} : "") +
($F{faxRecapito} != null ? ", n. fax " + $F{faxRecapito} : "") +
($F{emailRecapito} != null ? ", e-mail " + $F{emailRecapito} : "") +
($F{pecRecapito} != null ? ", e-mail <b>PEC</b> (posta elettronica certificata) " + $F{pecRecapito} : "") +
", con codice fiscale " + $F{codiceFiscale} +
" e partita IVA " + $F{partitaIVA} + ","]]></textFieldExpression>
			</textField>

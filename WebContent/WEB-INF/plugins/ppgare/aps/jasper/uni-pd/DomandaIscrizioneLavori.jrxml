						<font fontName="Arial" size="10"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA[($F{sesso} == "Maschio" ? "Il sottoscritto " : "La sottoscritta ") +
($F{cognome} == null ? "" : $F{cognome} + " ") + ($F{nome} == null ? "" : $F{nome}) +
($F{sesso} == "Maschio" ? ", nato il " : ", nata il ") + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascita}) +
" a " +
$F{comuneNascita} +
($F{provinciaNascita} == null ? "" : " (" + $F{provinciaNascita} + ")") +
", residente in " + $F{indirizzoResidenza} + " n. " + $F{numCivicoResidenza} + " cap " + $F{capResidenza} + " in " + $F{comuneResidenza} + ($F{provinciaResidenza}!= null ? " (" + $F{provinciaResidenza} + ")" : " - " + $F{nazioneResidenza}) +
 ", Codice Fiscale " + $F{codiceFiscaleFirmatario} +
 ", in qualità di " +
($P{LIBERO_PROFESSIONISTA}
    ? "Libero Professionista"
    : $F{qualifica} + " per " + $F{ragioneSociale} + ", con sede legale in " +
      $F{indirizzoSedeLegale} + " " + $F{numCivicoSedeLegale} +
      ", " + $F{capSedeLegale} + " " + $F{comuneSedeLegale} +
      ($F{provinciaSedeLegale} == null || $F{provinciaSedeLegale}.isEmpty() ? "$F{nazioneSedeLegale}" : " (" + $F{provinciaSedeLegale} + ") ") +
      ($F{partitaIVA} != null ? ", partita I.V.A n. " + $F{partitaIVA} : "") +
      ", Codice Fiscale " + $F{codiceFiscale} )]]></textFieldExpression>
				</textField>

					<textElement textAlignment="Justified">
						<font fontName="Arial" size="11"/>
					</textElement>
					<text><![CDATA[di impegnarsi, con la presentazione dell’offerta, ad offrire Prodotti conformi al Capitolato Tecnico, alle caratteristiche e specifiche tecniche indicate nel Catalogo;]]></text>
				</staticText>
				<staticText>
					<reportElement positionType="Float" x="0" y="0" width="35" height="29" isRemoveLineWhenBlank="true" isPrintWhenDetailOverflows="true"/>
					<textElement textAlignment="Left">

		<band height="227" splitType="Stretch">
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="0" width="483" height="25"/>
				<textElement textAlignment="Justified">
					<font fontName="Arial" size="11"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Il/La sottoscritto/a "
+
($P{LIBERO_PROFESSIONISTA}
    ? $F{ragioneSociale}
    : (($F{cognome} == null ? "____________________" : $F{cognome} + " ") + ($F{nome} == null ? "____________________" : $F{nome}))
) +
", nato/a a "
+
($P{LIBERO_PROFESSIONISTA}
    ? $F{comuneNascitaLiberoProfessionista}
    : $F{comuneNascita}
) +
($P{LIBERO_PROFESSIONISTA}
    ? ($F{provinciaNascitaLiberoProfessionista} == null ? " prov. ________"
      : " prov. " + $F{provinciaNascitaLiberoProfessionista})
    : ($F{provinciaNascita} == null ? " prov. ________"
        : " prov. " + $F{provinciaNascita})
) +
($P{LIBERO_PROFESSIONISTA}
    ? " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascitaLiberoProfessionista})
    : " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascita})
) +
($P{LIBERO_PROFESSIONISTA}
    ? ", in qualità di Libero Professionista"
    : ", in qualità di " + $F{qualifica} + " dell'operatore economico " + $F{ragioneSociale} + ", "
)]]></textFieldExpression>
			</textField>
			<frame>
				<reportElement positionType="Float" x="-1" y="25" width="483" height="5"/>
			</frame>
			<textField>
				<reportElement positionType="Float" x="-1" y="30" width="88" height="19"/>
				<textElement>
					<font fontName="Arial" size="11" isBold="false" isItalic="false" isUnderline="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["sede legale"]]></textFieldExpression>
			</textField>

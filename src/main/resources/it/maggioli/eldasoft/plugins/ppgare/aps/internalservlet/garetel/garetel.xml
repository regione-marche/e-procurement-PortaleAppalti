		<action name="openPageImportPrezziUnitari" class="openPageImportPrezziUnitariAction" method="openPage">
			<result type="dispatcher">
				/WEB-INF/plugins/ppgare/aps/jsp/internalServlet/garetel/prezzi_unitari/prezziUnitariImport.jsp

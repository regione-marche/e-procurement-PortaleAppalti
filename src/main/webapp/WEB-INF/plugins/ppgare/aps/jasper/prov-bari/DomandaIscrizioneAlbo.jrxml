    : ($F{provinciaNascita} == null ? " prov. ________"
        : " prov. " + $F{provinciaNascita})
) +
($P{LIBERO_PROFESSIONISTA}
    ? " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascitaLiberoProfessionista})
    : " il " + new SimpleDateFormat("dd/MM/yyyy").format($F{dataNascita})
) +
($P{LIBERO_PROFESSIONISTA}
    ? ", in qualitÃ  di Libero Professionista"
    : ", in qualitÃ  di " + $F{qualifica} + " dell'impresa " + $F{ragioneSociale} + ", "
)]]></textFieldExpression>
